{
  "name": "algo-visualizer-python",
  "displayName": "Algo visualizer (Python)",
  "description": "Visualize Python execution line-by-line. Made by Rohit Yadav, NIT Jalandhar.",
  "version": "1.0.1",  
  "publisher": "YADAV1825RohitYadav",
  "icon": "icon.jpg",    
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "viz.current",
        "title": "Open Algo Visualizer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "typescript": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YADAV1825/algo-viz" 
  }
}